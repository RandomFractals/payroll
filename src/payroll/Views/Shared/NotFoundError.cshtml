@{
    ViewData["Title"] = "404 - Resource Not Found Error";
}
<div class="panel panel-danger">
    <div class="panel-heading">
        <h3 class="panel-title">@ViewData["Title"]</h3>
    </div>
    <div class="panel-body">
        <p class="text-danger">Requested resource could not be located.</p>
        <p class="text-info">Please check your url and try again.</p>
    </div>
</div>
